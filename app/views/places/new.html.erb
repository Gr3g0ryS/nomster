<div class="well lg col-xs-10 col-xs-offset-1 col-md-6 col-md-offset-3">
  <h1>Enter Your Place Details</h1>
  <%= simple_form_for @place do |f| %>
  	<%= f.input :name %>
  	<%= f.input :address %>
  	<%= f.input :description %>
  	<br />
  	<%= f.submit 'Submit', :class => 'btn btn-primary' %>
  	<% end %>
</div>